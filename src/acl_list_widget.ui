<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkBox" id="top-level">
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkScrolledWindow">
      <property name="vscrollbar-policy">always</property>
      <property name="hscrollbar-policy">never</property>
      <property name="width-request">-1</property>
      <property name="height-request">150</property>
      <child>
        <object class="GtkColumnView" id="column-view">
          <property name="reorderable">False</property>
          <property name="hexpand">True</property>
          <property name="vexpand">True</property>
        </object>
      </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">horizontal</property>
        <property name="margin-top">8</property>
        <child>
          <object class="GtkImage" id="warning-icon">
            <property name="icon-name">dialog-warning</property>
            <property name="visible">false</property>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="warning-label">
            <property name="margin-start">8</property>
            <property name="label" translatable="yes">There are ineffective permissions</property>
            <property name="xalign">0</property>
            <property name="visible">false</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkBox">
        <property name="orientation">horizontal</property>
        <property name="halign">start</property>
        <property name="hexpand">0</property>
        <child>
          <object class="GtkToggleButton" id="edit-default-participants">
            <property name="label" translatable="yes">Edit default participants</property>
            <property name="margin-top">8</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
